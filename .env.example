# Database
DATABASE_URL=file:./sqlite.db

# Better Auth Configuration
BETTER_AUTH_URL=http://localhost:3000
BETTER_AUTH_SECRET=your-secret-key-here

# API Service Configuration
# This API key is used for server-to-server authentication with the Spring Boot backend
API_SERVICE_KEY=your-api-service-key-here

# API key header name (defaults to X-API-KEY if not specified)
API_HEADER_NAME=X-API-KEY

# Backend API URL
API_DOCS_URL=http://localhost:8080/v3/api-docs
